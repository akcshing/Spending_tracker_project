<h1>Transactions</h1>

  <a href="/transactions/new">Create</a>
  <div class="total">
    Total <%= @transactions_total %>
  </div>
  <table>
    <tr>
      <td>Amount</td>
      <td>Tag</td>
      <td>Merchant</td>
    </tr>
    <% @transactions.each do |transaction| %>
    <tr>
      <td><%= transaction.amount%></td>
      <td><%= transaction.tag_id%></td>
      <td><%= transaction.merchant_id%></td>
    </tr>
    <% end %>
  </table>
